<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student1" class="com.example.di.dependencyinjection_using_settermethod.Student">
        <property name="studentName" value="Anis"/>
        <property name="id" value="1"></property>
    </bean>

    <bean id="student2" class="com.example.di.dependencyinjection_using_settermethod.Student">
        <property name="studentName" value="Haroon"/>
        <property name="id" value="2" ></property>
    </bean>

    <bean id="student3" class="com.example.di.dependencyinjection_using_settermethod.Student">
        <property name="studentName" value="Waqar"/>
        <property name="id" value="3"></property>
    </bean>
    <bean id="student4" class="com.example.di.dependencyinjection_using_settermethod.Student">
        <property name="studentName" value="Ozair"/>
        <property name="id" value="4"></property>
    </bean>
</beans>